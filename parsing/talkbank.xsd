    <xs:attribute name="type">
      <xs:simpleType>
        <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
          <xs:documentation> Optional attribute for a word. </xs:documentation>
        </xs:annotation>

        <xs:restriction base="xs:string">
          <xs:enumeration value="action">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
              <xs:documentation>0</xs:documentation>
              <xs:documentation>
                <a href="http://childes.psy.cmu.edu/manuals/chat.html#UntranscribedMaterial_Code"
                  >CHAT manual section on this topic...</a>
              </xs:documentation>
            </xs:annotation>
          </xs:enumeration>

          <xs:enumeration value="omission">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
              <xs:documentation>0word</xs:documentation>
              <xs:documentation>
                <a href="http://childes.psy.cmu.edu/manuals/chat.html#OmittedWord_Code">CHAT manual
                  section on this topic...</a>
              </xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="ellipsis">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
              <xs:documentation>00word</xs:documentation>
              <xs:documentation>
                <a href="http://childes.psy.cmu.edu/manuals/chat.html#OmittedWord_Code">CHAT manual
                  section on this topic...</a>
              </xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="fragment">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
              <xs:documentation>&amp;; phonological fragment</xs:documentation>
              <xs:documentation>
                <a href="http://childes.psy.cmu.edu/manuals/chat.html#PhonologicalFragment_Code"
                  >CHAT manual section on this topic...</a>
              </xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>

